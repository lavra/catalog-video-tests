{
    "intelephense.telemetry.enabled": false,
    "php.suggest.basic": false,
    "intelephense.compatibility.correctForArrayAccessArrayAndTraversableArrayUnionTypes": false,
    "intelephense.environment.phpVersion": "8.1.8",
